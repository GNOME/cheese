msgstr "Chiều rộng của ảnh được chụp từ máy ảnh, theo điểm ảnh"

#: ../data/org.gnome.Cheese.gschema.xml.h:13
msgid "Photo height"
msgstr "Chiều cao ảnh"

#: ../data/org.gnome.Cheese.gschema.xml.h:14
